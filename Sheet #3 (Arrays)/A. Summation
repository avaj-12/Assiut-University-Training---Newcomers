#include <bits/stdc++.h>
#define fastio                        \
    ios_base::sync_with_stdio(false); \
    cin.tie(NULL);                    \
    cout.tie(NULL);

using namespace std;
int main()
{
    cin.tie(NULL);

    long long int n, sum = 0;
    cin >> n;
    long long int num[n];
    for (long long int i = 0; i < n; i++)
    {
        cin >> num[i];
        sum = sum + num[i];
    }
    cout << abs(sum) << endl;
    return 0;
}
