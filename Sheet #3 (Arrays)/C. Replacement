#include <bits/stdc++.h>
#define fastio                        \
    ios_base::sync_with_stdio(false); \
    cin.tie(NULL);                    \
    cout.tie(NULL);

using namespace std;
int main()
{
    cin.tie(NULL);

    int n;
    cin >> n;
    int num[n];
    for (int i = 0; i < n; i++)
    {
        cin >> num[i];
        if (num[i] > 0)
            num[i] = 1;
        else if (num[i] < 0)
            num[i] = 2;
        else
            num[i] = 0;

        cout << num[i] << " ";
    }

    return 0;
}
