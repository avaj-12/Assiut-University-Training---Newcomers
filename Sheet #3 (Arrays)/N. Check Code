#include <bits/stdc++.h>

using namespace std;

int main()
{
    int a, b, count = 0;
    cin >> a >> b;

    string s;
    cin >> s;

    for (int i = 0; i < s.size(); i++)
    {
        if (s[i] < '0' || s[i] > '9')
            count++;
    }
    // cout << count;
    if (s[a] == '-' && count == 1)
        cout << "Yes";
    else
        cout << "No";

    return 0;
}
